@model VehicleType;
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="text-uppercase text-center">Delete VehicleType</h2>
        </div>
    </div>
    <div class="row">
        <div class="col-md-8 border-1 border">
            <form method="post" enctype="multipart/form-data">
                <input asp-for="@Model.Id" hidden/>
            
                <div class="mb-3">
                    <label asp-for="@Model.Name" class="form-label"></label>
                    <input asp-for="@Model.Name" class="form-control" />
                   
                </div>
   
             
                <div class="mb-3">
                    <a asp-area="Admin" asp-controller="VehicleType" asp-action="Index" class="btn btn-warning ">Back</a>
                    <button type="submit" class="btn btn-danger">Delete</button>

                </div>

            </form>

        </div>
       

    </div>

</div>

